#xsetroot -solid "#00121A"

# enable bluetooth module for audio sink
pactl load-module module-bluetooth-discover

# set previous desktop background with nitrogen
nitrogen --restore

# activate compton compositor
compton -cCb --config ~/.config/compton.conf

# activate tint2 panel
tint2 &

# activate volti volume applet
volti &

# conkies
conky -c ~/.config/conky/conky.conf &
conky -c ~/.config/conky/conky_netmon &
conky -c ~/.config/conky/conky_loggs &
conky -c ~/.config/conky/conky_processes &
